{
    "name": "r64/nova-email-marketing-tool",
    "description": "A Laravel Nova tool to manage your email marketing lists",
    "keywords": [
        "laravel",
        "nova",
        "mailchimp",
        "activecampaign",
        "getresponse",
        "email marketing"
    ],
    "license": "MIT",
    "repositories": [
        {
            "type": "path",
            "url": "../packages/laravel-email-marketing"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "drewm/mailchimp-api": "^2.5",
        "r64/laravel-email-marketing": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "R64\\NovaEmailMarketingTool\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "R64\\NovaEmailMarketingTool\\NovaEmailMarketingServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
